<h2>Add Address</h2>
<%= form_for @address do |f| %>

<%= f.label :street %><br/>
<%= f.text_area :street, rows:15, cols: 50 %><br/>

<%= f.label :city %><br/>
<%= f.text_field :city %><br/>

<%= f.label :pin %> <br/>
<%= f.text_field :pin %><br/>

<%= f.label :title %><br/>
<%= f.text_field :title %><br/> 

<%= f.submit "submit" %>

<%end%>
